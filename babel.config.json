{
	"presets": [
		[
			"@babel/preset-env",
			{
				"targets": {
					"node": "current"
				}
			}
		],
		"@babel/preset-typescript"
	],
	"plugins": [
		[
			"module-resolver",
			{
				"alias": {
					"@configs": "./src/configs",
					"@controllers": "./src/controllers",
					"@models": "./src/models",
					"@routes": "./src/routes"
				}
			}
		]
	],
	"ignore": ["**/*.spec.ts"]
}
